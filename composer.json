{
  "name": "raorsa/rw-file-cache",
  "type": "library",
  "description": "RW File Cache is a PHP File-based Caching Library. Its syntax is designed to closely resemble the PHP memcache extension.",
  "keywords": [
    "cache",
    "caching",
    "file cache",
    "caching library",
    "library",
    "php"
  ],
  "homepage": "https://github.com/raorsa/RW-File-Cache",
  "license": "LGPL-3.0-only",
  "require": {
    "php": ">=8.2",
    "ext-zlib": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "php-coveralls/php-coveralls": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Raorsa\\RWFileCache\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
